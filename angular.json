{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "grmDashboard": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/grmDashboard",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/assets/plugins/css/materialize.min.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "src/assets/plugins/dataTable/datatables.min.css",
              "src/assets/plugins/css/jquery-ui.min.css",     
              "src/assets/plugins/css/animate.css",
              "node_modules/apexcharts/dist/apexcharts.css",
              "src/assets/plugins/toastr/toastr.min.css",
              "src/styles.scss"
            ],
            "scripts": [
              "src/assets/plugins/js/jquery.min.js",
              "src/assets/plugins/js/materialize.min.js",
              "src/assets/plugins/dataTable/datatables.min.js",
              "node_modules/feather-icons/dist/feather.min.js",
              "src/assets/plugins/js/jquery-ui.min.js",
              "src/assets/plugins/js/popper.min.js",
              "src/assets/plugins/js/loading.js",
              "src/assets/plugins/aes.js",
              "node_modules/bootstrap/dist/js/bootstrap.min.js",
              "node_modules/apexcharts/dist/apexcharts.min.js",
              "src/assets/plugins/js/common.js",
              "src/assets/plugins/toastr/toastr.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "5mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "10kb",
                  "maximumError": "25kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "grmDashboard:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "grmDashboard:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "grmDashboard:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "grmDashboard:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "grmDashboard:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "grmDashboard",
  "cli": {
    "analytics": "5aa55913-a5d0-4b32-a21f-97d657ec09d1"
  }
}